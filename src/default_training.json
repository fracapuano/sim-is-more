{
    "dataset": "cifar10",
    "searchspace": "nats",
    "target_device": "edgegpu",
    "algorithm": "PPO",
    "env_name": "oscar",
    "verbose": 1,
    "train_timesteps": 1000,
    "evaluate_every": 500,
    "history_len": 5,
    "test_episodes": 25,
    "seed": 777,
    "gamma": 0.6,
    "learning_rate": 3e-4,
    "n_envs": 3,
    "parallel_envs": false,
    "offline": true,
    "epsilon_scheduling": "const",
    "min_eps": 0.1,
    "max_eps": 0.3,
    "use_wandb_callback": true,
    "entropy_coeff": 0.01, 
    "normalization_type": "minmax"
}
